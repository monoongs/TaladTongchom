.parallax1,.parallax2,.parallax3,.parallax4,.parallax5,.parallax6,.parallax7,.parallax8,.parallax9,.parallax10,.parallax11,.parallax12 {
	position: relative;
	background-attachment: fixed;
	background-position: center;
	background-repeat: no-repeat;
	background-size: cover;
}

.parallax1 {
	background-image: url(../img/s1nl.png);
	min-height: 100%;
	opacity: 100;
}

